<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.biastester"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="22" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".BiasTester"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".IntroScreen"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_intro_screen"
            android:parentActivityName=".BiasTester"
            android:theme="@style/FullscreenTheme" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.biastester.BiasTester" />
        </activity>
        <activity
            android:name=".IntroScreen2"
            android:label="@string/title_activity_intro_screen2"
            android:parentActivityName=".BiasTester" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.biastester.BiasTester" />
        </activity>
        <activity
            android:name=".IntroScreen3"
            android:label="@string/title_activity_intro_screen3"
            android:parentActivityName=".BiasTester" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.biastester.BiasTester" />
        </activity>
        <activity
            android:name=".BeginPompt"
            android:label="@string/title_activity_begin_pompt"
            android:parentActivityName=".BiasTester" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.biastester.BiasTester" />
        </activity>
        <activity
            android:name=".Test"
            android:label="@string/title_activity_test" >
        </activity>
        <activity
            android:name=".Results"
            android:label="@string/title_activity_results" >
        </activity>
        <activity
            android:name=".TestSelect"
            android:label="@string/title_activity_test_select"
            android:parentActivityName=".BeginPompt" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.biastester.BeginPompt" />
        </activity>
        <activity
            android:name=".Results2"
            android:label="@string/title_activity_results2"
            android:parentActivityName=".Results" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.biastester.Results" />
        </activity>
        <activity
            android:name=".PositiveTest"
            android:label="@string/title_activity_positive_test" >
        </activity>
        <activity
            android:name=".SexualTest"
            android:label="@string/title_activity_sexual_test" >
        </activity>
        <activity
            android:name=".Results3"
            android:label="@string/title_activity_results3" >
        </activity>
        <activity
            android:name=".Results4"
            android:label="@string/title_activity_results4"
            android:parentActivityName=".Results2" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.biastester.Results2" />
        </activity>
        <activity
            android:name=".MultiTest"
            android:label="@string/title_activity_multi_test" >
        </activity>
    </application>

</manifest>
